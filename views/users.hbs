<main>
  <h1>List of Users</h1>
  {{#if hasUsers}}
  <table class="user-list">
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Phone Number</th>
    </tr>
    {{#each users}}
    <tr>
      <td>{{this.fname}}</td>
      <td>{{this.lname}}</td>
      <td>{{this.email}}</td>
      <td>{{this.phone}}</td>
    </tr>
    {{/each}}
  </table>
  {{else}}
  <h1>No Users Found!</h1>
  {{/if}}
</main>